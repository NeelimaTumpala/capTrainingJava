//constructor

//Account

package day2;

public class Account {
	    private int acno;
		private String name;
		private double balance;
		
		Account(){
			acno = 123;
			name = "neelima";
			balance = 50000;
			
		}
		
		public int getAcno() {
			return acno;
		}
		public void setAcno(int acno) {
			this.acno = acno;
		}
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
		public double getBalance() {
			return balance;
		}
		public void setBalance(double balance) {
			this.balance = balance;
		}
        
	

}


//Account Main

package day2;

public class AccoutMain {

	public static void main(String[] args) {
		
		Account acn = new Account();
		
		Account acn2 = new Account();
		System.out.println(acn.getAcno()+" "+acn.getName()+" "+acn.getBalance());
		System.out.println(acn2.getAcno()+" "+acn2.getName()+" "+acn2.getBalance());
	}

}


//customer

package day2;

public class Customer {
	private int code;
	private String name;
	public int getCode() {
		return code;
	}
	public void setCode(int code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	

}


//CustomerMain

package day2;

public class CustomerMain {

	public static void main(String[] args) {
		Customer cust = new Customer();
		cust.setCode(10);
		cust.setName("neelima");
		System.out.println(cust.getCode()+" "+cust.getName());

	}

}


//overriding

package day2;

public class Overloading {
	public void add(int x, int y) {
		System.out.println("int add "+(x+y));
	}
	public void add(double x, double y) {
		System.out.println("double add "+(x+y));
	}
	public void add(String x, String y) {
		System.out.println("string add "+(x+y));
	}
	public void add(float x, float y) {
		System.out.println("float add "+(x+y));
	}

	public static void main(String[] args) {
		Overloading load = new Overloading();
		load.add(5.6f,8.9f);
		
		

	}

}

//student

package day2;

public class Student {
	private int regno;
	private String studentname;
	private int sub1;
	private int sub2;
	private int sub3;
	public int getRegno() {
		return regno;
	}
	public void setRegno(int regno) {
		this.regno = regno;
	}
	public String getStudentname() {
		return studentname;
	}
	public void setStudentname(String studentname) {
		this.studentname = studentname;
	}
	public int getSub1() {
		return sub1;
	}
	public void setSub1(int sub1) {
		this.sub1 = sub1;
	}
	public int getSub2() {
		return sub2;
	}
	public void setSub2(int sub2) {
		this.sub2 = sub2;
	}
	public int getSub3() {
		return sub3;
	}
	public void setSub3(int sub3) {
		this.sub3 = sub3;
	}
	

}

//StudentMain

package day2;

public class StudentMain {

	public static void main(String[] args) {
		Student std = new Student();
		std.setRegno(Integer.parseInt(args[0]));
		std.setStudentname(args[1]);
		std.setSub1(Integer.parseInt(args[2]));
		std.setSub2(Integer.parseInt(args[3]));
		std.setSub3(Integer.parseInt(args[4]));
		
		double total=std.getSub1()+std.getSub2()+std.getSub2();
		double average = total/3;
		char grade;
		if(std.getSub1()>=60 && std.getSub2() >= 60 && std.getSub3()>=60) {
			grade = 'A';
			
		}
		else if(std.getSub1()>=50 && std.getSub2() >= 50 && std.getSub3()>=50) {
			grade = 'B';
			
		}
		else if(std.getSub1()>=40 && std.getSub2() >= 40 && std.getSub3()>=40) {
			grade = 'c';
			
		}
		else {
			grade = 'D';
		}
		
		
		System.out.println(std.getRegno()+" "+std.getStudentname()+" "+total+" "+average+" "+grade);
	    
		}

}


//polymorphism
//studentt
package day2;

public class Studentt {
	protected int regno;
	protected String name;
	protected String group;
	public Studentt() {
		regno = 101;
		name= "neelima";
		group = "EEE";
	}
	
	void dispaly() {
		System.out.println(regno+" "+name+" "+group);
	}
}


//library

package day2;

public class Library extends Studentt{
	private int booksTaken;
	public Library() {
		booksTaken =5;
	}
	void output() {
		System.out.println(regno+" "+name+" "+" "+group+" "+booksTaken);
	}
	public static void main(String[] args) {
		Library lib = new Library();
		lib.output();

	}

}

//using parametrized constructor in super class-polymorphism

//student
package day2;

public class Studentt {
	protected int regno;
	protected String name;
	protected String group;
	
	
	public Studentt(int regno, String name, String group) {
		super();
		this.regno = regno;
		this.name = name;
		this.group = group;
	}


	void display() {
		System.out.println(regno+" "+name+" "+group);
	}
}

//libray 
//using parametrized constructor

package day2;

public class Studentt {
	protected int regno;
	protected String name;
	protected String group;
	
	
	public Studentt(int regno, String name, String group) {
		super();
		this.regno = regno;
		this.name = name;
		this.group = group;
	}


	void display() {
		System.out.println(regno+" "+name+" "+group);
	}
}

//inheritance multilevel

//stud
package day2;

public class Stud {
	protected int regno;
	protected String name;
	protected String group;
	public Stud(int regno, String name, String group) {
		
		this.regno = regno;
		this.name = name;
		this.group = group;
	}
	
	void display() {
		System.out.println("\nRegno: "+regno);
		System.out.println("Name: "+name);
		System.out.println("Group: "+group);
	}
	
	

}

//Examination

package day2;

public class Examination extends Stud {
	protected int sub1;
	protected int sub2;
	protected int sub3;
	Examination(int regno, String name, String group, int sub1, int sub2, int sub3) {
		super(regno, name, group);
		this.sub1 = sub1;
		this.sub2 = sub2;
		this.sub3 = sub3;
	}
	void marks() {
		int total = sub1+sub2+sub3;
		double average =total/3;
		if(sub1>40 && sub2>40 && sub3>40) {
			System.out.println("Total: "+total+"\nAverage: "+average);
			System.out.println("pass");
			
		}
		else {
			System.out.println("fail");
		}
    
	
	}
	

}

//Placement

package day2;

public class Placement extends Examination {
	private int interviewAttended;
	private int status;

	Placement(int regno, String name, String group, int sub1, int sub2, int sub3,int interviewAttended,int status) {
		super(regno, name, group, sub1, sub2, sub3);
		this.interviewAttended=interviewAttended;
		this.status=status;
	}
	void interview() {
		
		System.out.println("Interview Attended:"+interviewAttended);
		System.out.println("Interview selected: "+status);
	}

	public static void main(String[] args) {
		Placement obj = new Placement(204,"Neelima","EEE",98,97,96,6,3);
		obj.display();
		obj.marks();
		obj.interview();
	}

}

//inhertace student
//studentt -> Library
package day2;

public class Studentt {
	protected int regno;
	protected String name;
	protected String group;
	public Studentt(int regno, String name, String group) {
		super();
		this.regno = regno;
		this.name = name;
		this.group = group;
	}
	void display() {
		System.out.println("This is from super class");
	}
}

//Library

package day2;

public class Library extends Studentt{
	private int booksTaken;
	
	public Library(int regno, String name, String group, int booksTaken) {
		super(regno, name, group);
		this.booksTaken = booksTaken;
	}
	void output() {
		System.out.println(regno+" "+name+" "+" "+group+" "+booksTaken);
	}
	public static void main(String[] args) {
		Library lib = new Library(101,"neelima","EEE",6);
		lib.output();
		lib.display();

	}

}

//method overriding

//studentt -> Library
package day2;

public class Studentt {
	protected int regno;
	protected String name;
	protected String group;
	public Studentt(int regno, String name, String group) {
		super();
		this.regno = regno;
		this.name = name;
		this.group = group;
	}
	public void display() {
		System.out.println("This is from super class");
	}
}

//library

package day2;

public class Library extends Studentt{
	private int booksTaken;
	
	public Library(int regno, String name, String group, int booksTaken) {
		super(regno, name, group);
		this.booksTaken = booksTaken;
	}
	void output() {
		System.out.println(regno+" "+name+" "+" "+group+" "+booksTaken);
	}
	public void display() {
		super.display();
		System.out.println("This is from sub class");
	}
	public static void main(String[] args) {
		Library lib = new Library(101,"neelima","EEE",6);
		lib.output();
		lib.display();

	}

}

