//test array

package samplejavaproject;

public class TestArray {

	public static void main(String[] args) {
		int x[] = new int[5];
		int y[] = {4,3,5,6,7,8,2};
		for(int i=0;i<5;i++) {
			x[i] = i+2;
			
		}
		
		for(int i=0;i<5;i++) {
			System.out.println(y[i]+" ");
		}
		int a[][] = new int[3][3];
		int b[][] = {{1,2,3},{4,5,6}};
		for(int i=0;i<3;i++)
			for(int j=0;j<3;j++)
				a[i][j]=i+j;
		//to display
						
		for(int i=0;i<3;i++) {
			for(int j=0;j<3;j++)
				System.out.print(a[i][j]+" ");
		    System.out.println();
		
				
				

	}

}
}


//type casting


package samplejavaproject;

public class TypeCasting {

	public static void main(String[] args) {
		byte b=101;
		int x=b;
		System.out.println(x);//implicit casting
		int a=32765;
		short s=(short) a;//explicit casting
		System.out.println(s);
		
		
		

	}

}


//command line args

package samplejavaproject;

class CommandLineArgs {

	public static void main(String[] args) {
		String name =args[0];
		System.out.println(args[1]+args[2]);
		int sub1 = Integer.parseInt(args[1]);
		int sub2 = Integer.parseInt(args[2]);
		int total = sub1+sub2;
		System.out.println(total)
		

	}

}

